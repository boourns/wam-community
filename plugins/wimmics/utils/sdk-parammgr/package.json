    "@webaudiomodules/sdk": "^0.0.9",
    "dts-bundle-generator": "^6.2.0",
    "esbuild": "^0.14.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  },
  "bugs": {
    "url": "https://github.com/webaudiomodules/sdk-parammgr/issues"
